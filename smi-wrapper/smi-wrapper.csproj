    <TargetFramework>net6.0</TargetFramework>
    <RootNamespace>smi_wrapper</RootNamespace>
    <ImplicitUsings>disable</ImplicitUsings>
    <Nullable>enable</Nullable>
  </PropertyGroup>

  <ItemGroup>
